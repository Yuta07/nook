{
	"plugins": ["@typescript-eslint"],
	"extends": [
		"next",
		"next/core-web-vitals",
		"plugin:@typescript-eslint/recommended",
		"prettier",
		"plugin:import/recommended",
		"plugin:import/warnings"
	],
	"rules": {
		"import/order": [
			"error",
			{
				"alphabetize": {
					"order": "asc"
				}
			}
		]
	}
}
