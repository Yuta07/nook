{
  "name": "nook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --ext 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "next lint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.22.2",
    "@supabase/supabase-js": "^1.24.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "eslint-plugin-import": "^2.25.2",
    "next": "11.1.2",
    "next-seo": "^4.28.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.43.2"
  },
  "devDependencies": {
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/code": "^2.7.0",
    "@editorjs/embed": "^2.4.6",
    "@editorjs/header": "^2.6.1",
    "@editorjs/image": "^2.6.1",
    "@editorjs/inline-code": "^1.3.1",
    "@editorjs/list": "^1.6.2",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/quote": "^2.4.0",
    "@editorjs/warning": "^1.2.0",
    "@next/eslint-plugin-next": "^11.1.2",
    "@types/react": "17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "*.{js, jsx, ts, tsx, json, md}": [
      "yarn lint:fix",
      "yarn prettier:fix"
    ]
  }
}
